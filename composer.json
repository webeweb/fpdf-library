    "keywords": ["webeweb", "library", "FPDF", "PDF"],
    "homepage": "https://github.com/webeweb/fpdf-library",
    "license": "LGPL-3.0-only",
    "authors": [
        {
            "name": "webeweb",
            "homepage": "https://github.com/webeweb/"
        },
        {
